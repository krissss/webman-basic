{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "http://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "http://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "http://wenda.workerman.net/",
    "wiki": "http://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "illuminate/container": "^8.83",
    "illuminate/database": "^8.83",
    "illuminate/events": "^8.83",
    "illuminate/pagination": "^8.83",
    "illuminate/redis": "^8.83",
    "kriss/webman-auth": "^1.1",
    "kriss/webman-docker": "^1.2",
    "kriss/webman-eloquent-ide-helper": "^1.0",
    "kriss/webman-lock": "^1.0",
    "kriss/webman-logger": "^1.0",
    "monolog/monolog": "^2.0",
    "mouyong/validate": "^2.0",
    "psr/container": "v1",
    "robmorgan/phinx": "^0.12.10",
    "symfony/cache": "^5.4",
    "symfony/translation": "^5.4",
    "tinywan/exception-handler": "^0.1.0",
    "vlucas/phpdotenv": "^5.4",
    "webman/console": "^1.0",
    "webman/redis-queue": "^1.1",
    "workerman/crontab": "^1.0",
    "workerman/webman-framework": "^1.3.12",
    "yiisoft/json": "^1.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.4"
  },
  "suggest": {
    "ext-event": "For better performance. ",
    "ext-redis": "For redis use"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ],
    "phinx": "phinx"
  },
  "repositories": {
  }
}
