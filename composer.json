{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "https://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "https://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "illuminate/container": "^8.83",
    "illuminate/database": "^8.83",
    "illuminate/events": "^8.83",
    "illuminate/http": "^8.83",
    "illuminate/pagination": "^8.83",
    "illuminate/redis": "^8.83",
    "league/flysystem": "~1.1",
    "robmorgan/phinx": "^0.12.10",
    "symfony/cache": "^5.4",
    "symfony/process": "^5.4",
    "symfony/translation": "^5.4",
    "vlucas/phpdotenv": "^5.4",
    "webman-tech/amis-admin": "^2.0",
    "webman-tech/auth": "^2.0",
    "webman-tech/docker": "^2.0",
    "webman-tech/laravel-filesystem": "^2.1",
    "webman-tech/laravel-http-client": "^1.1",
    "webman-tech/laravel-translation": "^1.0",
    "webman-tech/laravel-validation": "^1.1",
    "webman-tech/log-reader": "^2.0",
    "webman-tech/logger": "^2.0",
    "webman-tech/polyfill": "^1.0",
    "webman-tech/symfony-lock": "^2.0",
    "webman/console": "^1.2",
    "webman/event": "^1.0",
    "webman/multi-session": "^1.0",
    "webman/redis-queue": "^1.2",
    "workerman/webman-framework": "^1.4",
    "yiisoft/json": "^1.0",
    "yiisoft/strings": "^2.0",
    "zircote/swagger-php": "^4.4"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.4",
    "webman-tech/debugbar": "^2.0"
  },
  "suggest": {
    "ext-event": "For better performance",
    "ext-redis": "For redis use",
    "workerman/crontab": "For crontab use",
    "workbunny/webman-nacos": "For nacos use"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app"
    },
    "files": [
      "./support/helpers.php",
      "./support/functions.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ],
    "phinx": "phinx"
  },
  "repositories": {
  }
}
